    "df_sample=pd.read_csv('/kaggle/input/playground-series-s4e6/sample_submission.csv')\n",
    "\n",
    "\n",
    "check_columns(df, df_test), print(),print()\n",
    "\n",
    "check_columns_dtype(df,df_test),print()\n",
    "\n",
    "to_predict(df_sample), print(), print()\n",
    "\n",
